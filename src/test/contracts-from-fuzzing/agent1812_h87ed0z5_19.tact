contract ManyBoolDefaults {
    a: Bool = false;
    b: Bool = true;
    c: Bool = false;
    d: Bool = true;
    e: Bool = false;
    f: Bool = true;
    g: Bool = false;
    h: Bool = true;

    init() {
        self.a = true;
        self.b = false;
        self.c = true;
        self.d = false;
        self.e = true;
        self.f = false;
        self.g = true;
        self.h = false;
    }
}