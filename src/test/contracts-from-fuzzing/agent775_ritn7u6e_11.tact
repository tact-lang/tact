contract OptionalsAndStrings {
    optInt: Int? = null;
    optBool: Bool? = null;
    optAddress: Address? = null;
    optString: String? = null;
    optCell: Cell? = null;
    optSlice: Slice? = null;

    init() {
        self.optInt = null;
        self.optBool = null;
        self.optAddress = null;
        self.optString = null;
        self.optCell = null;
        self.optSlice = null;
    }
}