contract AllInOne {
    intVal: Int = 42;
    boolVal: Bool = true;
    optInt: Int? = null;
    optBool: Bool? = false;
    strVal: String = "hello";
    optStr: String? = null;

    init() {
        self.intVal = 42;
        self.boolVal = true;
        self.optInt = null;
        self.optBool = false;
        self.strVal = "hello";
        self.optStr = null;
    }
}