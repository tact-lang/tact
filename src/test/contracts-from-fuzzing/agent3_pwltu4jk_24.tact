// Many optional booleans contract
contract ManyOptBools {
    o1: Bool?;
    o2: Bool?;
    o3: Bool?;
    o4: Bool?;
    o5: Bool?;
    o6: Bool?;
    o7: Bool?;
    o8: Bool?;
    o9: Bool?;
    o10: Bool?;

    init() {
        self.o1 = true;
        self.o2 = null;
        self.o3 = false;
        self.o4 = null;
        self.o5 = true;
        self.o6 = false;
        self.o7 = null;
        self.o8 = true;
        self.o9 = false;
        self.o10 = null;
    }
}