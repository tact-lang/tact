contract NestedAscriptions {
    i1: Int as int8;
    i2: Int as int16;
    i3: Int as int32;
    i4: Int as int64;
    i5: Int as uint8;
    i6: Int as uint16;
    i7: Int as uint32;
    i8: Int as uint64;

    init() {
        self.i1=1; self.i2=2; self.i3=3; self.i4=4;
        self.i5=5; self.i6=6; self.i7=7; self.i8=8;
    }
}
