contract BitPacked {
    a: Int as uint1;
    b: Int as uint1;
    c: Int as uint1;
    d: Int as uint1;
    e: Int as uint1;
    f: Int as uint1;
    g: Int as uint1;
    h: Int as uint1;
    i: Int as uint1;
    j: Int as uint1;

    init() {
        self.a=1; self.b=0; self.c=1; self.d=1; self.e=0; self.f=0; self.g=1; self.h=0; self.i=1; self.j=0;
    }
}