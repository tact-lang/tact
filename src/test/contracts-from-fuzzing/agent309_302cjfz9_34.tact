contract BooleanParty {
    aa: Bool = true;
    bb: Bool = false;
    cc: Bool = true;
    dd: Bool = false;
    ee: Bool = true;
    ff: Bool = false;
    gg: Bool = true;

    init() {
        self.aa = false;
        self.bb = true;
        self.cc = false;
        self.dd = true;
        self.ee = false;
        self.ff = true;
        self.gg = false;
    }
}